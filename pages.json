{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
			"path": "pages/index/index",
			"style": {
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/account/account"
		},
		{
			"path": "pages/bills/bills",
			"style": {
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/mine/mine"
		},
		{
			"path": "pages/accountDetail/accountDetail"
		},
		{
			"path": "pages/login/login"
		},
		{
			"path": "pages/billDetail/billDetail"
		},
		{
			"path": "uview-ui/components/u-avatar-cropper/u-avatar-cropper",
			"style": {
				"navigationBarTitleText": " 图标裁剪",
				"navigationBarBackgroundColor": "#000000"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "YAccount-您的记账小助手",
		"navigationBarBackgroundColor": "#2b85e4",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#909399",
		"selectedColor": "#000000",
		"list": [{
				"iconPath": "static/icon/index.png",
				"selectedIconPath": "static/icon/index_selected.png",
				"text": "首页",
				"pagePath": "pages/index/index"
			},
			{
				"iconPath": "static/icon/加号.png",
				"selectedIconPath": "static/icon/加号_selected.png",
				"text": "记账",
				"pagePath": "pages/account/account"
			},
			{
				"iconPath": "static/icon/bills.png",
				"selectedIconPath": "static/icon/bills_selected.png",
				"text": "账单",
				"pagePath": "pages/bills/bills"
			},
			{
				"iconPath": "static/icon/mine.png",
				"selectedIconPath": "static/icon/mine_selected.png",
				"text": "我的",
				"pagePath": "pages/mine/mine"
			}
		]
	}
}
